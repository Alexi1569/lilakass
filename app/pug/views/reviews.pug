            .previews__form-inputs
              .previews__form-left
                .form__group
                  .form__group-input
                    label(for="rf-name").form__group-label Имя
                    input(type="text" id="rf-name")
                  span.form__group-message Ошибка
                .form__group
                  .form__group-input
                    label(for="rf-email").form__group-label Email
                    input(type="email" id="rf-email")
                  span.form__group-message Ошибка
              .previews__form-right
                .form__group
                  .form__group-input
                    label(for="pr-text").form__group-label Ваш отзыв
                    textarea(id="pr-text" placeholder="Введите Ваш отзыв...")
                  span.form__group-message Ошибка
            button.btn.btn--linear.previews__form-btn
              span Оставить отзыв
